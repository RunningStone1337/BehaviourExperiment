    \u043E\u0437\u043D\u0430\u0447\u0430\u044E\u0442, \u0447\u0442\u043E \u0447\u0435\u043B\u043E\u0432\u0435\u043A
    \u0441\u0442\u0440\u0435\u043C\u0438\u0442\u0441\u044F \u043A \u043E\u0431\u0449\u0435\u043D\u0438\u044E,
    \u043B\u0435\u0433\u043A\u043E \u0438 \u0431\u044B\u0441\u0442\u0440\u043E, \u0447\u0430\u0441\u0442\u043E
